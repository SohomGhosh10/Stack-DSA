#include <iostream>
#include <stack>
using namespace std;
int main()
{
    string str;
    getline(cin , str);

    stack<char> s1;
    stack<char> s2;

    for(char ch : str)
        s1.push(ch);
    
    while(!s1.empty()){
        cout << s1.top();
        s1.pop();
    }

    cout << endl;

    return 0;
}
