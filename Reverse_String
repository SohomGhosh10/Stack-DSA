#include <iostream>
#include <stack>

using namespace std;

int main()
{
    string str;
    getline(cin,str);

    stack<char> s1;
    stack<char> s2;

    for (char ch : str)
        s1.push(ch);

    while (!s1.empty())
    {
        s2.push(s1.top());
        s1.pop();
    }

    while (!s2.empty())
    {
        cout << s2.top();
        s2.pop();
    }

    return 0;
}
